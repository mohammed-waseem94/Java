<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="950.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Main">
   <children>
      <ToolBar fx:id="toolbar" layoutY="8.0" prefHeight="50.0" prefWidth="1500.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <items>
          <Button mnemonicParsing="false" onAction="#homeclick" text="Home" />
            <MenuButton fx:id="product" mnemonicParsing="false" text="Product">
              <items>
                <MenuItem mnemonicParsing="false" text="Action 1" />
                <MenuItem mnemonicParsing="false" text="Action 2" />
              </items>
            </MenuButton>
            <MenuButton fx:id="setup" mnemonicParsing="false" text="Setup">
              <items>
                <MenuItem mnemonicParsing="false" text="Action 1" />
                <MenuItem mnemonicParsing="false" text="Action 2" />
              </items>
            </MenuButton>
            <Button mnemonicParsing="false" onAction="#aboutclick" text="About" />
            <Button mnemonicParsing="false" onAction="#helpclick" text="Help" />
            <Button mnemonicParsing="false" text="Reports" />
            <Button mnemonicParsing="false" onAction="#logoutclick" text="logout" />
        </items>
      </ToolBar>
      <Pane fx:id="homepane" layoutY="58.0" prefHeight="900.0" prefWidth="1500.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
   </children>
</AnchorPane>
